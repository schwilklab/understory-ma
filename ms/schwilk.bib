@Article{Zettler+Spira+etal-2001,
  Title                    = {Ant-seed mutualisms: can red imported fire ants sour the relationship?},
  Author                   = {Zettler, J. A. and Spira, T. P. and Allen, C. R.},
  Journal                  = {Biological Conservation},
  Year                     = {2001},
  Number                   = {2},
  Pages                    = {249-253},
  Volume                   = {101},

  Abstract                 = {Invasion by the red imported fire ant, Solenopsis invicta, has had negative impacts on individual animal and plant species, but little is known about how S. invicta affects complex mutualistic relationships. In some eastern forests of North America, 30% of herbaceous species have ant-dispersed seeds. We conducted experiments to determine if fire ants are attracted to seeds of these plant species and assessed the amount of scarification or damage that results from handling by fire ants. Fire ants removed nearly 100% of seeds of the ant-dispersed plants \emph{Trillium undulation}, \emph{T. discolor}, \emph{T. catesbaei}, \emph{Viola rotundifolia}, and \emph{Sanguinaria canadensis}. In recovered seeds fed to ant colonies, fire ants scarified 80% of \emph{S. canadensis} seeds and destroyed 86% of {V. rotundifolia} seeds. Our study is the first to document that red imported fire ants are attracted to and remove seeds of species adapted for ant dispersal. Moreover, fire ants might damage these seeds and discard them in sites unfavorable for germination and seedling establishment. (C) 2001 Elsevier Science Ltd. All rights reserved.},
  Annote                   = {Clemson Univ, Dept Biol Sci, 132 Long Hall, Clemson, SC 29634 USA ; Clemson Univ, Dept Biol Sci, Clemson, SC 29634 USA ; Clemson Univ, US Geol Survey, S Carolina Cooperat Fish \& Wildlife Res Unit, Clemson, SC 29634 USA English Article Bibliography Items: 36 Number of times cited: 0 (c) 2001 Inst. For Sci. Info},
  Keywords                 = {Solenopsis Invicta ; Red Imported Fire Ant ; Ant-Plant Mutualism ; Myrmecochory ; Seed Dispersal ; Invasive Species ; Carex-Pedunculata ; Dispersal ; Hymenoptera ; Formicidae ; Predation ; Behavior ; Invasion ; Viola}
