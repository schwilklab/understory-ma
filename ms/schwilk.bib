@Article{Goncharenko+Padutov+etal-1992,
  Title                    = {Population structure gene diversity and differentiation in natural populations of cedar pines (\emph{Pinus Subsect. Cembrae} Pinaceae) in the USSR},
  Author                   = {Goncharenko, G. G. and Padutov, V. E. and Silin, A. E.},
  Journal                  = {Plant Systematics and Evolution},
  Year                     = {1992},
  Number                   = {3-4},
  Pages                    = {121-134},
  Volume                   = {182},

  Abstract                 = {Twelve natural populations of four cedar pine species, Pinus sibirica, P. cembra, P. pumila, and P. koraiensis, occurring in the Soviet Union were investigated by starch-gel electrophoresis. Frequencies of 55 alleles at 19 loci were determined. Interpopulation genetic diversity in P. sibirica and P. pumila was only 2-4 percent of the total genetic diversity. Nei's distance coefficient (Dn) was used to estimate the level of genetic differentiation among conspecific populations and among species. Dn values among populations ranged from 0.006 to 0.038. A dendrogram constructed using Dn values divided cedar pines species into 2 clusters: sibirica-cembra (Dn = 0.030) and pumila-koraiensis (Dn = 0.143). Nei's distance between these clusters was 0.232. On the basis of the data obtained it was possible to draw the following conclusion: P. sibirica, P. pumila, and P. koraiensis are distinct species, while P. cembra should apparently be regarded as a geographical P. sibirica race.},
  Annote                   = {Dep. mol. genetics, byelorussian forestry res. inst., 71 proletarskaya str., 246654 gomel, byelorussia. 19 loci. Mean Fst for P. sibirica = 0.020 (Fis=-0.018, F-IT = 0.003, Gst = 0.027), Mean Fst for P. pumila = 0.037 (GST = 0.044), suggests phylogeny of pumila paired with koriensis and cembra paired with sibirica, then join both groups},
  Keywords                 = {pinus-sibirica pinus-cembra pinus-pumila pinus-koraiensis dendrogram species status. *Genetics and Cytogenetics/Plant [03504] *Genetics and Cytogenetics/Population Genetics (1972- ) [03509] *Botany, General and Systematic/Gymnospermae [50518] Botany, General and Systematic/Floristics and Distribution [50528] Coniferopsida [25102] Plants. Vascular Plants. Spermatophytes. Gymnosperms.}
