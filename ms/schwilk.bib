@Article{Casenave+Meloni+etal-1998,
  Title                    = {Seasonal variation in root carbohydrate reserves of three shrubs in the Chaco Occidental (Argentina)},
  Author                   = {Casenave, E. C. and Meloni, D. A. and Toselli, M. E.},
  Journal                  = {Biological Research},
  Year                     = {1998},
  Number                   = {2},
  Pages                    = {93-97},
  Volume                   = {31},

  Abstract                 = {In the phytogeographic area known as Chaco Occidental (Argentina), agroforestry systems applying different ways of management are under evaluation. To increase the capacity of art area to support more animals per unit area, shrubs are removed by clearcutting at ground level. Among these shrubs are atamisqui (Capparis atamisquea O. Ktze., Capparidaceae), jarilla (Larrea divaricata Cav., Zygophyllaceae), and teatin (Acacia furcatispina Burk., Leguminosae). These shrubs are remarkable because they have high density, frequency and coverage, high resprouting capability and fast biomass recovery. However, there is very little information about them. The evaluation of the carbohydrate dynamics in native shrubs of the Chaco Occidental region has not been studied yet. Knowledge of seasonal variation of root carbohydrates in these species may be an important aspect to consider in the timing of clearcutting practice. In this study, three individuals of each species were sampled monthly, and starch and soluble sugars were determined. Starch plus soluble sugar comprise the total non-structural carbohydrate (TNC) fraction. Results showed that the minimal TNC was observed during flowering for atamisqui and teatin, and during fruiting for jarilla, which coincided with the high rainfall of the early Summer. There were significant (P<0.01) differences in the TNC levels between dry- and wet season-atamisqui (257 and 183 mu g/mg dry weight), jarilla (196 and 135) and teatin (303 and 231), respectively. Therefore, the time of low TNC and consequently the best time to cut occurs during the time of highest precipitation, when plants have exhausted carbohydrate reserves from their roots to resprout and flower.},
  Annote                   = {Univ Nacl Santiago Del Estero,Fac Agron \& Agroind,Lab Fisiol Vegetal,Belgrano S 1912,Ra-4200 Santiago Estero,Argentina English Article Bibliography Items: 23 Number of times cited: 0 (c) 2000 Inst. For Sci. Info},
  Keywords                 = {Acacia Furcatispina ; Capparis Atamisquea ; Carbohydrate Reserves ; Larrea Divaricata ; Root ; Shrubs ; Dynamics ; Growth ; Trees}
}

