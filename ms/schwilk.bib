@Article{Abrams-1977,
  Title                    = {{Density-Independent} Mortality and Interspecific Competition: A Test of Pianka's Niche Overlap Hypothesis},
  Author                   = {Peter A. Abrams},
  Journal                  = {The American Naturalist},
  Year                     = {1977},

  Month                    = jun,
  Note                     = {{ArticleType:} research-article / Full publication date: May - Jun., 1977 / Copyright 1977 The University of Chicago Press},
  Number                   = {979},
