@Article{Rainey+Travisano-1998,
  Title                    = {Adaptive radiation in a heterogeneous environment},
  Author                   = {Rainey, P. B. and Travisano, M.},
  Journal                  = {Nature},
  Year                     = {1998},
  Number                   = {6688},
  Pages                    = {69-72},
  Volume                   = {394},

  Abstract                 = {Successive adaptive radiations have played a pivotal role in the evolution of biological diversity(1-3). The effects of adaptive radiation are often seen(4-6), but the underlying causes are difficult to disentangle and remain unclear(7-9). Here we examine directly the role of ecological opportunity and competition in driving genetic diversification. We use the common aerobic bacterium Pseudomonas fluorescens(10), which evolves rapidly under novel environmental conditions to generate a large repertoire of mutants(11-13). When provided with ecological opportunity (afforded by spatial structure), identical populations diversify morphologically, but when ecological opportunity is restricted there is no such divergence. In spatially structured environments, the evolution of variant morphs follows a predictable sequence and we show that competition among the newly evolved niche-specialists maintains this variation. These results demonstrate that the elementary processes of mutation and selection alone are suifficient to promote rapid proliferation of new designs and support the theory that trade-offs in competitive ability drive adaptive radiation(14,15).},
  Annote                   = {Univ Oxford,Dept Plant Sci,S Parks Rd,Oxford Ox1 3rb,England English Article Bibliography Items: 30 Number of times cited: 41 (c) 2001 Inst. For Sci. Info},
  Keywords                 = {Evolution ; Adaptation ; Bacteria ; Diversification ; Polymorphism ; Populations ; Divergence ; Tests}
}

