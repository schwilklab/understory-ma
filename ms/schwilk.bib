@Article{Poulos+Taylor+etal-2007,
  Title                    = {Environmental controls on dominance and diversity of woody plant species in a Madrean, Sky Island ecosystem, Arizona, {USA}},
  Author                   = {Poulos, H. M. and Taylor, A. H. and Beaty, R. M.},
  Journal                  = {Plant Ecology},
