@Article{Martinez+LopezPortillo-2003,
  Title                    = {Growth and Architecture of Small Honey Mesquites under Jackrabbit Browsing: Overcoming the Disadvantage of being Eaten},
  Author                   = {{ARMANDO} J. {MARTÃNEZ} and {JORGE} {LÃPEZâPORTILLO}},
  Journal                  = {Annals of Botany},
  Year                     = {2003},
  Number                   = {3},
  Pages                    = {365 --375},
  Volume                   = {92},

  Abstract                 = {Browsing is an important mortality factor in seedlings and small plants. However, the induced changes in the architecture of plant survivors may influence subsequent browsing, opening the possibility of compensating for the damage done. How jackrabbit {(Lepus} californicus) browsing affects the growth and architecture of small individuals of honey mesquite, Prosopis glandulosa var. torreyana, a tree/shrub that produces spines at every node, was explored. Naturally established mesquites of unknown age were selected in one site, and 2âyearâold mesquites were transplanted in another site. In both cases, half of them were exposed to jackrabbits and the other half were excluded as controls. After 4 years, shoot production (height, length and number of derived shoots) and plant growth (height and cover) increased 1Â·4-2Â·5âfold in naturally established controls relative to exposed plants, depending on the measured variable. In the transplant experiment, the increases were 2Â·8-7Â·1âfold in controls relative to exposed plants 2 years after initiation of the experiment. The net loss of biomass in treatment vs. control plants in these experiments suggests a negative response to browsing which has been defined as underâcompensation. Alternative architectures in honey mesquites were evident at the end of the exclusion experiments: controls had long branches and an extended crown cover, while exposed plants had short branches and a compact crown cover. Results indicated that mesquites were able to grow under browser pressure by packing many stems in a compact matrix armed with spines and producing one or more shoots tall and wide enough to escape from jackrabbits.},
  Doi                      = {10.1093/aob/mcg148},
  File                     = {Martinez+LopezPortillo-2003-mesquite_browsing_architecture.pdf:plains_savannas/Martinez+LopezPortillo-2003-mesquite_browsing_architecture.pdf:PDF},
  Owner                    = {schwilk},
  Shorttitle               = {Growth and Architecture of Small Honey Mesquites under Jackrabbit Browsing},
  Timestamp                = {2010.10.07},
  Url                      = {http://aob.oxfordjournals.org/content/92/3/365.abstract}
}

