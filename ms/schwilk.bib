@Article{Outcalt+Wade-2004,
  Title                    = {Fuels management reduces tree mortality from wildfires in southeastern {United} States},
  Author                   = {K.W. Outcalt and D.D. Wade},
  Journal                  = {Southern Journal Of Applied Forestry},
  Year                     = {2004},
