@Article{Cowling+Bond-1991,
  Title                    = {How small can reserves be: an empirical approach in {Cape} fynbos {South Africa}},
  Author                   = {Cowling, R. M. and Bond, W. J.},
  Journal                  = {Biological Conservation},
  Year                     = {1991},
  Number                   = {3},
  Pages                    = {243-256},
  Volume                   = {58},

  Abstract                 = {A comparison of the species-area relations of south-western Cape limestone Fynbos islands (surrounded by Fynbos on acid sand) and equivalent-sized areas of mainland indicated a weak fragmentation effect on overall species richness. There was no difference between mainland and island floras in the frequency of species belonging to different dispersal types, pollination syndromes, growth forms and shrub height categories. This probably results from islands being exposed to the same disturbance (fire) regime and being accessible to similar pollination and dispersal vectors as the mainland. A fragmentation effect was also diluted by the establishment on islands of edaphic generalists from the adjacent acid sand Fynbos. Species most vulnerable to extinction on small limestone fragments were locally endemic and limestone-specialist shrubs. Our results suggest that the minimum reserve size needed to avoid species losses (including local endemics) is about 4-15 ha. This is encouraging since c. 90% of existing reserves in the transformed lowland Fynbos landscapes of the south-western Cape are larger than 15 ha. The results of empirically based studies such as this are likely to be locality-specific. Nonetheless, they do provide meaningful guidelines for reserve design and improve our understanding of fragmentation-related extinction patterns.},
  Annote                   = {Bot. dep., univ. cape town, private bag, rondebosch 7700, south africa. Using Smart Source Parsing s English. Article.},
  Keywords                 = {plant animal pollination species dispersal disturbance regime species richness habitat fragmentation effect reserve design landscape conservation. *General Biology/Conservation, Resource Management [00512] *Ecology; Environmental Biology/Plant [07506] *Ecology; Environmental Biology/Animal [07508] *Plant Physiology, Biochemistry and Biophysics/Reproduction [51512] Plantae-Unspecified [11000] Animalia-Unspecified [33000] Plants. Animals.}
}

